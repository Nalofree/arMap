        strong Объекты недвижимости
      .b_objects.container-fluid
        -for (var i = 0; i < 10; i++)
          .col-md-4.col-lg-3.col-sm-6.col-xs-12
            .b_objects-item
              a(href='/offices')
                .b_objects-item-img
                  img(src= img, alt='')
                .b_objects-item-heading= heading
                .b_objects-item-description= description
      .clearfix
  .load-next
    a(href='#') ↓ Далее
  footer
    a(name='footer')
    include ./footer.jade